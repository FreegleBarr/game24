[application]

config/name="Game 24"
run/main_scene="res://Battle/Arenas/Eye.tscn"
boot_splash/use_filter=false
config/icon="res://Icons/icon.png"
config/windows_native_icon="res://Icons/icon.ico"

[autoload]

Battle="*res://Battle/autoload.gd"

[display]

window/size/width=1280
window/size/height=720
window/stretch/mode="2d"
window/stretch/aspect="expand"

[editor_plugins]

enabled=PoolStringArray( "res://addons/dialogic/plugin.cfg" )

[importer_defaults]

texture={
"compress/bptc_ldr": 0,
"compress/hdr_mode": 0,
"compress/lossy_quality": 0.7,
"compress/mode": 0,
"compress/normal_map": 0,
"detect_3d": false,
"flags/anisotropic": false,
"flags/filter": false,
"flags/mipmaps": false,
"flags/repeat": 0,
"flags/srgb": 2,
"process/HDR_as_SRGB": false,
"process/fix_alpha_border": true,
"process/invert_color": false,
"process/normal_map_invert_y": false,
"process/premult_alpha": false,
"size_limit": 0,
"stream": false,
"svg/scale": 1.0
}

[layer_names]

2d_physics/layer_1="Walls Battle"
2d_physics/layer_2="Projectile Battle"

[physics]

common/enable_pause_aware_picking=true
